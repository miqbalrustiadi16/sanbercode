import loginPage from '../support/page/loginPage'; // pastikan path ini benar

describe('Login Test with POM and Fixture', () => {
  beforeEach(() => {
    loginPage.visit();
  });

  it('should login successfully using fixture credentials', () => {
    cy.fixture('credentials').then(({ username, password }) => {
      loginPage.enterUsername(username);
      loginPage.enterPassword(password);
      loginPage.clickLogin();
    });

    loginPage.verifyLoginSuccess();
  });
});
